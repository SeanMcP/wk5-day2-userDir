{{#userData}}
  <a href="/listing/{{id}}">
    <div class="userCard">
      <img src={{avatar}} alt={{name}}/>
      <h2>{{name}}</h2>
      {{#job}}
      <p>{{job}}, {{company}}</p>
      {{/job}}
      {{^job}}
      <p class='hire'>Available for hire</p>
      {{/job}}
    </div>
  </a>
{{/userData}}
